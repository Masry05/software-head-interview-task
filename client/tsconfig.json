{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": false,
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {},
    "types": ["vite/client", "react", "react-dom"]
  },
  "include": ["src", "vite.config.js"],
  "exclude": ["node_modules", "dist"]
}
